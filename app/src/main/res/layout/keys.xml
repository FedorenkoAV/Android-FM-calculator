<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        ads:adSize="BANNER"
        ads:adUnitId="@string/banner_ad_unit_id"
        app:layout_constraintTop_toTopOf="parent"></com.google.android.gms.ads.AdView>

    <LinearLayout
        android:id="@+id/main_display"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/keys"
        android:background="@color/lcd_background"
        android:orientation="vertical"
        android:paddingTop="50dp"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:background="@color/colorLCDBackground"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:layout_gravity="center_horizontal"
                android:background="@color/colorLCDBackground">

                <ru.fmproject.android.calculator.TextViewPlus

                    android:id="@+id/status_shift"
                    style="@style/status"
                    android:text="SHIFT" />

                <ru.fmproject.android.calculator.TextViewPlus
                    android:id="@+id/status_hyp"
                    style="@style/status"
                    android:text="HYP" />

                <ru.fmproject.android.calculator.TextViewPlus
                    android:id="@+id/status_deg"
                    style="@style/status"
                    android:text="DEG" />

                <ru.fmproject.android.calculator.TextViewPlus
                    android:id="@+id/status_rad"
                    style="@style/status"
                    android:text="RAD" />

                <ru.fmproject.android.calculator.TextViewPlus
                    android:id="@+id/status_grad"
                    style="@style/status"
                    android:text="GRAD" />

                <ru.fmproject.android.calculator.TextViewPlus
                    android:id="@+id/status_bracket"
                    style="@style/status"
                    android:text="( )" />

                <ru.fmproject.android.calculator.TextViewPlus
                    android:id="@+id/status_bin"
                    style="@style/status"
                    android:text="BIN" />

                <ru.fmproject.android.calculator.TextViewPlus
                    android:id="@+id/status_oct"
                    style="@style/status"
                    android:text="OCT" />

                <ru.fmproject.android.calculator.TextViewPlus
                    android:id="@+id/status_hex"
                    style="@style/status"
                    android:text="HEX" />

                <ru.fmproject.android.calculator.TextViewPlus
                    android:id="@+id/status_cplx"
                    style="@style/status"
                    android:text="CPLX" />

                <ru.fmproject.android.calculator.TextViewPlus
                    android:id="@+id/status_sd"
                    style="@style/status"
                    android:text="SD" />

            </LinearLayout>

            <ru.fmproject.android.calculator.TextViewPlus
                android:id="@+id/protocol"
                style="@style/protocol"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/linearLayout"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"

                android:paddingTop="2dp"
                android:paddingRight="10dp"
                android:singleLine="true"
                tools:text="(-0.1+2)-3*4/56789=÷^√πʸ√x³√°ʹʺeˣxʸ∑×%" />

            <ru.fmproject.android.calculator.TextViewPlus
                android:id="@+id/display"
                style="@style/display"
                android:layout_below="@+id/protocol"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_margin="0dp"
                android:clickable="true"
                android:focusable="true"
                android:isScrollContainer="false"
                android:maxLines="1"
                android:paddingStart="0dp"
                android:paddingLeft="0dp"
                android:paddingTop="0dp"
                android:paddingEnd="10dp"
                android:paddingRight="10dp"
                android:paddingBottom="10dp"
                android:text="@string/_888888888888"
                android:textSize="@integer/l_text_size" />

            <ru.fmproject.android.calculator.TextViewPlus
                android:id="@+id/memory"
                style="@style/mem_err"
                android:layout_below="@+id/protocol"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:text="M" />

            <ru.fmproject.android.calculator.TextViewPlus
                android:id="@+id/error"
                style="@style/mem_err"
                android:layout_alignBottom="@+id/display"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:text="E" />

        </RelativeLayout>
    </LinearLayout>

